<%= flash[:notice] %>
<h2>learner</h2>
<%= form_with url:"/learner/new_learner",method: :post do |form| %>
<div class="field">
    <%= form.label :name %>
    <%= form.text_field :name%>
</div>

<div class="field">
    <%= form.label :age %>
    <%= form.number_field :age%>
</div>

<div class="field">
    <%= form.label :marks1 %>
    <%= form.number_field :marks1%>
</div>

<div class="field">
    <%= form.label :marks2 %>
    <%= form.number_field :marks2%>
</div>

<div class="field">
    <%= form.label :marks3 %>
    <%= form.number_field :marks3%>
</div>
<div class="field">
    <%= form.label :marks4 %>
    <%= form.number_field :marks4%>
</div>
<div class="field">
    <%= form.label :marks5 %>
    <%= form.number_field :marks5 %>
</div>
<div class="field">
    <%= form.label :grade %>
    <%= form.text_field :grade%>
</div>

<div class="actions">
    <%= form.submit "learner login" %>
</div>
<% end %>